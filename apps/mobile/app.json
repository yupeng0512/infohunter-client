{
  "expo": {
    "name": "InfoHunter",
    "slug": "infohunter",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "scheme": "infohunter",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F172A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.infohunter.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F172A"
      },
      "package": "com.infohunter.app",
      "edgeToEdgeEnabled": true
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "autolinkingModuleResolution": true,
      "reactCanary": true
    }
  }
}
